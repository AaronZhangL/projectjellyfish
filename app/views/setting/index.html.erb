<h1>Settings</h1>
<h3><%= link_to 'New Setting', '/setting/new' %></h3>
<% @settings.each do |setting| %>
    <div>
      <%= link_to setting.name, "/setting/#{setting.id}" %>
      -
      <%= link_to 'Edit', "/setting/#{setting.id}/edit" %>
      <%= link_to 'Delete', "/setting/#{setting.id}", method: :delete, 'data-confirm' => 'Are you sure?' %>
    </div>
<% end %>
